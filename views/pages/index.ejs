<%- include('../modules/header'); %>

<main>
  <div class="greeting">
    <p id="welcome">Welcome to ACM.</p>
    <a href="/register">Join us</a>
    <a target="_blank" href="#">Calendar</a>
  </div>
  <div class="featured">
    <img class="featured-img" src="media/blockchain.png" alt="What is the Blockchain?">
  </div>
</main>
<script src="https://unpkg.com/typewriter-effect/dist/core.js"></script>
<script type="text/javascript">
  var app = document.getElementById('welcome');

  var typewriter = new Typewriter(app, {
      loop: true
  });

  typewriter.typeString('Welcome to ACM.')
      .pauseFor(5500)
      .deleteAll()
      .typeString('Let\'s code.')
      .pauseFor(2500)
      .start();
</script>
